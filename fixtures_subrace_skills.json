        "model": "api.ReferenceBookSubraceItemSkill",
        "fields": {
            "name": "Проворство полуросликов",
            "description": "Вы можете проходить сквозь пространство, занятое существами, чей размер больше вашего.",
            "skill_book_id": 1
        }
    },
    {
        "pk": 16,
        "model": "api.ReferenceBookSubraceItemSkill",
        "fields": {
